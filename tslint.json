{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-console": false,
    "jsx-boolean-value": ["never"],
    "no-var-requires": false,
    "max-classes-per-file": false
  }
}
