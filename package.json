  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,css,md}": [
      "prettier --write --print-width 120 --single-quote --trailing-comma es5",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --fix -t verbose",
      "prettier --write --print-width 120 --single-quote --parser typescript --trailing-comma es5",
      "git add"
    ]
  }
